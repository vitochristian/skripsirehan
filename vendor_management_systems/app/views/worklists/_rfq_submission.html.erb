<table>
  <thead>
    <tr>
      <th colspan='8' class='title'>RFQ SUBMISSIONS</th>
    </tr>
    <tr>
      <th>RFQ ID</th>
      <th>ITEM NAME</th>
      <th>DESCRIPTION</th>
      <th>QUANTITY</th>
      <th>CREATED DATE</th>
      <th>EXPIRATION DATE</th>
    </tr>
  </thead>
  <tbody>    
    <% @tr_rfq_submissions.each do |rfq_submission| %> 
      <tr>
        <td><%#= rfq_submission.no_rfq %></td> <!-- bapak -->
        <td><%#= rfq_submission.item_name %></td> <!-- bapak -->
        <td><%= rfq_submission.description %></td> 
        <td><%#= rfq_submission.qty %></td> <!-- bapak -->
        <td><%= rfq_submission.created_at %></td> 
        <td><%#= rfq_submission.expiration_date %></td> <!-- bapak -->
      </tr>
    <% end %>
  </tbody>
</table>